[loader]
entrypoint = "file:/usr/local/lib/x86_64-linux-gnu/gramine/libsysdb.so"
log_level = "error"
insecure__use_cmdline_argv = true
insecure__use_host_env = true
pal_internal_mem_size = "128M"

[loader.env]
LD_LIBRARY_PATH = "/lib:/usr/lib:/lib/x86_64-linux-gnu:/usr//lib/x86_64-linux-gnu"
OMP_NUM_THREADS = "8"

[libos]
entrypoint = "/usr/bin/python3.8"

[[fs.mounts]]
uri = "file:/usr/local/lib/x86_64-linux-gnu/gramine/runtime/glibc"
path = "/lib"

[[fs.mounts]]
uri = "file:/lib/x86_64-linux-gnu"
path = "/lib/x86_64-linux-gnu"

[[fs.mounts]]
uri = "file:/usr"
path = "/usr"

[[fs.mounts]]
uri = "file:/etc"
path = "/etc"

[[fs.mounts]]
uri = "file:/home/guiaraujo/CACIC-DevKIt/use_case/database/"
path = "/database"

[[fs.mounts]]
uri = "file:/home/guiaraujo/.local/lib/python3.8/site-packages/"
path = "/home/guiaraujo/.local/lib/python3.8/site-packages/"

[[fs.mounts]]
type = "tmpfs"
path = "/tmp"

[sgx]
enclave_size = "4G"
max_threads = 32
edmm_enable = false
nonpie_binary = true
trusted_files = [
    { uri = "file:/usr/local/lib/x86_64-linux-gnu/gramine/libsysdb.so" },
    { uri = "file:/usr/bin/python3.8" },
    { uri = "file:/usr/local/lib/x86_64-linux-gnu/gramine/runtime/glibc/" },
    { uri = "file:/lib/x86_64-linux-gnu/" },
    { uri = "file:/usr//lib/x86_64-linux-gnu/" },
    { uri = "file:/usr/lib/python3.8/" },
    { uri = "file:/usr/lib/python3/dist-packages/" },
    { uri = "file:/home/guiaraujo/.local/lib/python3.8/site-packages/" },
    { uri = "file:/home/guiaraujo/.local/lib/python3.8/site-packages/" },
    { uri = "file:/usr/local/lib/python3.8/" },
    { uri = "file:/home/guiaraujo/CACIC-DevKIt/gramine_training_tests/main.py" },
    { uri = "file:/home/guiaraujo/CACIC-DevKIt/use_case/database/sample_dataset.csv" },
    { uri = "file:/home/guiaraujo/CACIC-DevKIt/use_case/database/saved_model" },
]
isvprodid = 0
isvsvn = 0
remote_attestation = "none"
debug = false
require_avx = false
require_avx512 = false
require_mpx = false
require_pkru = false
require_amx = false
require_exinfo = false
enable_stats = false
